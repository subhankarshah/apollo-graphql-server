{
	"restartable": "rs",
	"ignore": [
		".git",
		"node_modules/**/node_modules"
	],
	"verbose": true,
	"execMap": {
		"ts": "node --require ts-node/register"
	},
	"watch": [
		"src/"
	],
	"env": {
		"NODE_ENV": "development",
		"HASURA_END_POINT": "http://localhost:8080/v1/graphql",
		"HASURA_ADMIN_SECRET": "DummySecret"
	},
	"ext": "js,json,ts"
}